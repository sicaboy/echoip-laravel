{
    "name": "sicaboy/echoip-laravel",
    "description": "Laravel Client for echoip",
    "keywords": [
        "sicaboy",
        "laravel",
        "echoip-laravel"
    ],
    "homepage": "https://github.com/sicaboy/echoip-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "sicaboy",
            "email": "sica@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
                "Sicaboy\\EchoipLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
                "Sicaboy\\EchoipLaravel\\Tests\\": "tests"
        }
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                    "Sicaboy\\EchoipLaravel\\EchoipLaravelServiceProvider"
            ],
            "aliases": {
                    "EchoipLaravel": "Sicaboy\\EchoipLaravel\\Facades\\EchoipLaravel"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
